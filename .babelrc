{
    "presets": [
      ["@babel/env", {
        "targets": {
          "node": "current"
        }
      }]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      ["module-resolver", {
        "alias": {
          "@Src": "./src",
          "@Database": "./src/database",
          "@Routes": "./src/routes",
          "@Utils": "./src/utils",
          "@StatusCodes": "./src/utils/statusCodes",
          "@Knexfile": "./knexfile.js"
        }
      }]
    ]
  }